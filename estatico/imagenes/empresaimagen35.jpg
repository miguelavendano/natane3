		<script>
		function cambiarNumeroHabitaciones(){
			$("#texto_adunin").css("display","none");
			$(".linea_habitacion").hide();
			max_habitaciones = parseInt($("#frmReserva input[name=max_habitaciones]").val());
			num_habi =$("#numero_habitaciones").val();
			if (num_habi == "") {
				num_habi=0;
			}
			if (num_habi>0) {
				for (c=0;c<num_habi;c++){
					if (c==0) $("#texto_adunin").css("display","table-row");
					$("#habi"+c).css("display","table-row");
				}
			}
		}
