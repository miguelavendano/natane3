			mostrar_reservas();
			var res=false;
			if ($("#frmReserva input[name=fecha_entrada]").val()=="") {
				alert("Para reservar es necesario que seleccione una fecha de entrada.");
				$("#frmReserva input[name=fecha_entrada]").focus();
			}
			else if ($("#frmReserva input[name=fecha_salida]").val()=="") {
				alert("Para reservar es necesario que seleccione una fecha de salida.");
				$("#frmReserva input[name=fecha_salida]").focus();
			}
			else if ($("#frmReserva input[name=fecha_entrada]").val()==$("#frmReserva input[name=fecha_salida]").val()) {
				alert("La fecha de salida es igual o anterior a la fecha de entrada.");
				$("#frmReserva input[name=fecha_salida]").focus();
			}
			else if ($("#frmReserva input[name=numero_habitaciones]").val()=="" || $("#frmReserva input[name=numero_habitaciones]").val()=="0") {
				alert("Para reservar es necesario que indique el número de habitaciones.");
				$("#frmReserva input[name=numero_habitaciones]").focus();
			}
			else if ($('#frmReserva input:radio[name=pension]:checked').map(function () { return this.value;}).get() == "")  {
				alert("Para reservar es necesario que seleccione un tipo de pensión.");
			}
			else {
				//Comprobar adultos
				val = parseInt($("#numero_habitaciones").val());
				var puedo=true;
				for(c=0;c<val;c++){
					val2 = parseInt($("#adultos"+c).val())
					if (isNaN(val2)){
						alert("Para reservar es necesario que indique los adultos de la habitación");
						$("#adultos"+c).focus();
						puedo=false;
						return false;
					}else if(val2==0){
						alert("Para reservar es necesario que indique los adultos de la habitación");
						$("#adultos"+c).focus();
						puedo=false;
						return false;
					}
				}
				if (puedo) res=true;
			}
			return res;
		}
		</script>
		
		
